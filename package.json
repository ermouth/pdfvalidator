{
  "name": "pdfvalidator",
  "version": "0.2.0",
  "description": "A signed PDF validator",
  "main": "build/index.js",
  "scripts": {
    "build": "babel src --compact=false --out-dir build --source-maps",
    "generate-docs": "jsdoc --destination doc/ src/index.js",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fotisl/pdfvalidator.git"
  },
  "keywords": [
    "PKI",
    "X.509",
    "PDF",
    "WebCrypto"
  ],
  "author": "Fotis Loukos <me@fotisl.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/fotisl/pdfvalidator/issues"
  },
  "homepage": "https://github.com/fotisl/pdfvalidator#readme",
  "dependencies": {
    "asn1js": "2.0.15",
    "pkijs": "2.0.67",
    "node-webcrypto-ossl": "1.0.31"
  },
  "devDependencies": {
    "babel-cli": "latest",
    "babel-preset-es2015": "latest",
    "jsdoc": "3.5.5"
  },
  "browser": {
    "./build/webcrypto.js": false
  }
}
